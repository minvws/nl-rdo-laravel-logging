{
  "name": "minvws/laravel-logging",
  "description": "Laravel provider for logging functionality",
  "keywords": [
    "laravel",
    "logging"
  ],
  "type": "library",
  "license": "EUPL",
  "authors": [
    {
      "name": "Rick Lambrechts",
      "email": "rick@rl-webdiensten.nl"
    },
    {
      "name": "Joshua Thijssen",
      "email": "jthijssen@noxlogic.nl"
    }
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0|^11.0",
    "nuwber/rabbitevents": "^8.0",
    "nesbot/carbon": "^2.72 || ^3.0",
    "minvws/audit-logger": "^0.4.0",
    "ext-sodium": "*"
  },
  "require-dev": {
    "orchestra/testbench": "^8.22",
    "phpunit/phpunit": "^9.5 || ^10.0",
    "vimeo/psalm": "^5.0",
    "phpstan/phpstan": "^1.7 || ^2.0",
    "squizlabs/php_codesniffer": "^3.9",
    "mockery/mockery": "^1.4.3",
    "phpmd/phpmd": "^2.15"
  },
  "autoload": {
    "psr-4": {
      "MinVWS\\Logging\\Laravel\\": "src/"
    },
    "classmap": [
      "src/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MinVWS\\Logging\\Laravel\\Tests\\": "tests/"
    }
  },
  "suggest": {
    "nuwber/rabbitevents": "For writing log records to rabbitMQ"
  },
  "extra": {
    "laravel": {
      "providers": [
        "MinVWS\\Logging\\Laravel\\LogServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": [
        "vendor/bin/phpcs",
        "vendor/bin/phpunit",
        "vendor/bin/phpstan analyse",
        "vendor/bin/psalm"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  },
  "repositories": {
    "minvws/audit-logger": {
      "type": "vcs",
      "url": "git@github.com:minvws/nl-rdo-php-audit-logger.git"
    }
  }
}
